;@¶"&/performance/clever-conditionals/{;{	;"¿// old way
    if (type == 'foo' || type == 'bar') { ... }

    // better
    if (/^(foo|bar)$/.test(type)) { ... }

    // object literal lookup
    if (({ foo : 1, bar : 1 })[type]) { ... };	"í<p>// old way
    if (type == &lsquo;foo&rsquo; || type == &lsquo;bar&rsquo;) { &hellip; }</p>

<pre><code>// better
if (/^(foo|bar)$/.test(type)) { ... }

// object literal lookup
if (({ foo : 1, bar : 1 })[type]) { ... }
</code></pre>
;
"c/<!DOCTYPE html> 
 
